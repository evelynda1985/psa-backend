{
  "connection": "default",
  "collectionName": "checkings",
  "info": {
    "name": "checkings"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "checkingDate": {
      "required": true,
      "type": "date"
    },
    "event": {
      "model": "event"
    },
    "parent": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "hour": {
      "type": "time"
    }
  }
}
